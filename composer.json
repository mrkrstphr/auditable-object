  "license": "MIT",
  "authors": [
    {
      "name": "Kristopher Wilson",
      "email": "kristopherwilson@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "mrkrstphr\\AuditableObject\\": "src/"
    }
  },
  "require-dev": {
    "peridot-php/peridot": "^1.19",
    "peridot-php/leo": "^1.6",
